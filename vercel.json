{
    "version": 2,
    "builds": [
      {
        "src": "collatask-frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "collatask-backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/routes/(.*)",
        "dest": "/collatask-backend/routes/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/collatask-frontend/$1"
      }
    ]
  }
  